{
  "name": "mongodb-stage-validator",
  "description": "Parses and validates MongoDB aggregation pipeline stages.",
  "author": "Anna Herlihy <anna@mongodb.com>",
  "version": "0.2.1",
  "homepage": "https://github.com/mongodb-js/stage-validator",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/stage-validator"
  },
  "dependency-check": {
    "entries": [
      "./index.js"
    ],
    "ignore": [
      "debug"
    ]
  },
  "license": "Apache-2.0",
  "scripts": {
    "ci": "npm run check && npm test",
    "test": "npm run compile && mocha --compilers js:babel-register",
    "compile": "npm run babel && npm run pegjs",
    "babel": "babel ./src --out-dir ./lib",
    "pegjs": "pegjs -o ./lib/parser.js < ./src/agg_pipeline.pegjs",
    "check": "npm run compile && mongodb-js-precommit ./src ./test",
    "prepublishOnly": "npm run compile",
    "pretest": "npm run compile"
  },
  "main": "index.js",
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bson": "^4.0.2",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.2.0",
    "eslint-config-mongodb-js": "^5.0.3",
    "mocha": "^7.0.0",
    "mongodb-js-precommit": "^2.0.0"
  }
}
